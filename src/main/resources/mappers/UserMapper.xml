<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.zjgsforum.Dao.UserMapper">
    <select id="getUserId" resultType="java.lang.Integer">
        select user_id from zjgsforum.user where account = #{account} and password = #{password}
    </select>
</mapper>